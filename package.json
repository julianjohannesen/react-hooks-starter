{
  "name": "react-hooks-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "test": "yarn run eslint && yarn run jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint src/. --fix-dry-run --config .eslintrc.json",
    "eslint:fix": "eslint src/. --fix --config .eslintrc.json",
    "mocha": "mocha test/ --recursive --exit",
    "start": "NODE_PATH=src/adapters react-scripts start",
    "watch": "NODE_PATH=src/adapters react-scripts start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "raven-js": "^3.27.0",
    "react": "16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-hooks-global-state": "^0.4.1",
    "react-icons": "^3.2.2",
    "react-notifications-component": "^1.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-hash-link": "^1.2.1",
    "react-scripts": "2.1.1",
    "rollbar": "^2.5.1",
    "styled-components": "^4.1.2",
    "styled-system": "^3.1.11",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "jest-cli": "^23.6.0",
    "jest-localstorage-mock": "^2.3.0",
    "react-fix-it": "^0.3.3",
    "why-did-you-update": "^1.0.6"
  }
}
